{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/common-config/(.*)": "<rootDir>/../libs/common-config/src/$1",
    "@app/common-config": "<rootDir>/../libs/common-config/src",
    "@app/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@app/logger": "<rootDir>/../libs/logger/src",
    "@app/services/(.*)": "<rootDir>/../libs/services/src/$1",
    "@app/services": "<rootDir>/../libs/services/src",
    "@app/utilities/(.*)": "<rootDir>/../libs/utilities/src/$1",
    "@app/utilities": "<rootDir>/../libs/utilities/src"
  }
}